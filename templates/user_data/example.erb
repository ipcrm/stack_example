#!/bin/bash

yum install -y httpd24 php70 mysql56-server php70-mysqlnd
chkconfig httpd on
service httpd start

MYSQL=<%= attr!("#{iname}_r_d_s_d_b_instance".to_sym, "Endpoint.Address".to_sym) %>
echo $MYSQL > /etc/mysql

echo "
Hello world!
<br />
<br />
<b>MySQL:</b> ${MYSQL}
" > /var/www/html/index.html



